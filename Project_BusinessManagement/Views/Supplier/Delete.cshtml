@model Project_BusinessManagement.Models.MSupplier

@{
    ViewBag.Title = "Eliminar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="bg-warning">
    @if (Model.LMessageException != null)
    {
        <div class="alert alert-danger">
            <strong>Error!</strong> @Model.LMessageException
        </div>
    }

</div>
<h2>Eliminar</h2>

<h3>Esta Seguro de Eliminar el Proveedor?</h3>
<div>
    
    <h4>Proveedor</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.LIdSupplier)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LIdSupplier)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.LNameSupplier)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LNameSupplier)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.LNoIdentification)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LNoIdentification)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.LCreationDate)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.LCreationDate)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar" class="btn btn-danger" /> |
            @Html.ActionLink("Ir a Lista De Proveedores", "Index")
        </div>
    }
</div>
